#!/usr/bin/env perl

use strict;
use warnings;

use lib "/opt/c7a-perl/current/cmd/local/lib/perl5";
use FindBin;
use lib "$FindBin::RealBin/../lib";

# Remember to use `SKIP_SYS_RUNALONE=1 /opt/tdr/current/sbin/press` to skip this test...
use Sys::RunAlone silent => 1;
use Getopt::Long;
use CIHM::Meta::Press;

my $conf = "/etc/canadiana/tdr/tdr.conf";
my $skip = 0;
GetOptions (
    'conf:s' => \$conf,
    'skip:i' => \$skip
    );


CIHM::Meta::Press->new({ 
    configpath => $conf, skip => $skip
                            })->Press;

1;
__END__
